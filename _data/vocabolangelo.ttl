vocang:burrigliata
    a skos:Concept ;
    skos:prefLabel "Burrigliata"@it ;
    skos:definition "Una grigliata in cui sono anche presenti dei burrito."@it ;
    skos:example "Venerdì ci sei alla burrigliata?"@it;
    dct:created "2022-08-24" ;
    dct:creator vocang:bdeouiEddy .

vocang:griglito
    a skos:Concept ;
    skos:prefLabel "Griglito"@it ;
    skos:definition "Sinonimo maschile di Burrigliata."@it ;
    skos:example "Venerdì ci sei al griglito?"@it;
    dct:created "2022-08-24" ;
