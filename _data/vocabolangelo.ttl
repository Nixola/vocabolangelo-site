vocang:VocabolangeloRDF
    a owl:Ontology ;
    owl:imports <http://www.perceive.net/schemas/20021119/relationship/relationship.rdf> ;
    rdfs:label "Vocabolangelo RDF" ;
    rdfs:comment "Un'ontologia creata per il Vocabolangelo, utilizzata per memorizzare le parole,  definizioni, persone, relazioni etc."@it ;
    rdfs:seeAlso "https://github.com/AngeloFilaseta/vocabolangelo-rdf" .

# Classes

vocang:Game a owl:Class .

# Properties

vocang:unpaidFriendOf
    a owl:ObjectProperty ;
    rdfs:subPropertyOf foaf:knows ;
    rdfs:domain foaf:Person ;
    rdfs:range foaf:Person ;
    rdfs:label "Unpaid friend of", "Amico non ricambiato"@it;
    rdfs:comment "Property to be used when the person specified in the domain is friend with the person specified in the range, but the friendship is not mutual.", "Proprietà da utilizzare quando la persona specificata nel dominio è amica di quella specificata nel range, ma l'amicizia non è ricambiata."@it .

scot:synonym
    a owl:SymmetricProperty ;
    rdfs:subPropertyOf skos:related.

