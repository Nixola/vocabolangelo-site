    skos:example "Sono povero, pago con lo scontante."@it, "Sono 1400€ se paghi col bancomat, 1200€ con lo scontante"@it;
    dct:creator vocang:orlandoNicola .

vocang:coscionchetta
    a skos:Concept ;
