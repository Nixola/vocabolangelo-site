vocang:arsenzio
    a skos:Concept ;
    skos:prefLabel "Arsenzio"@it ;
    skos:definition "Assenzio con dentro arsenico."@it ;
    skos:example "Stasera mi faccio un bicchierino di Arsenzio."@it ;
